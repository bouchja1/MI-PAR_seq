#include "TaskCrate.h"

TaskCrate::TaskCrate(bool* resultingConfigurationArray, unsigned int width,
        unsigned int height, unsigned int iterations, unsigned int aMistake) {
    this->resultingConfigurationArray=resultingConfigurationArray;
    this->aMistake=aMistake;
    this->width=width;
    this->height=height;
    this->iterations=iterations;
}

TaskCrate::~TaskCrate() {
    delete resultingConfigurationArray;
}
